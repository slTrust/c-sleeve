.container{
    box-sizing: border-box;
    width: 710rpx;
    border: 2rpx dashed #DCEBE6;
    display: flex;
    flex-direction: column;
    padding: 5rpx 25rpx;
    background-color: #fff;
}

.inner{
    height: 50rpx;
    border-bottom: 2px solid #DCEBE6;
    display: flex;
    flex-direction: column;
    justify-content: center;
    font-size: 24rpx;
    color: #333333;
}
